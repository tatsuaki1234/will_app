<div class="contents row">
  <p><%= @nickname %>さんの投稿一覧</p>
  <div class="mypage-index">
    <%= @articles.each do |article| %>
      <div class="content_post" style="background-image: url(<%= article.image %>);">
        <p><%= article.title %></p>
        <span class="name"><%= article.content %></span>
      </div>
    </div>
  <% end %>
</div>